{
  "name": "webstarter",
  "version": "0.1.2",
  "description": "A complete web starter project built on modern frontend tech",
  "license": "SEE LICENSE IN <LICENSE.md>",
  "main": "index.js",
  "engines": {
    "node": ">=7.4",
    "npm" : ">=4"
  },
  "scripts": {
    "build:dev": "gulp webpack",
    "build:dist": "cross-env NODE_ENV=production webpack -p",
    "watch": "cross-env NODE_ENV=development gulp webpack-watch",
    "predev": "npm run build:dev",
    "dev": "cross-env NODE_ENV=development PORT=5000 node --harmony-async-await index.js",
    "prestart": "npm run build:dist",
    "start": "cross-env NODE_ENV=production node --harmony-async-await index.js",
    "preversion": "npm test",
    "postversion": "sh scripts/deploy.sh ",
    "lint": "eslint ./src --ext=.js,.jsx",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel-core/register ./test/test/**/*"
  },
  "keywords": [
    "webstarter",
    "inferno",
    "koa",
    "webpack"
  ],
  "author": "Sven Anders Robbestad <robbestad@gmail.com> (https://www.robbestad.com/)",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-inferno": "^1.9.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2040": "^1.1.1",
    "cross-env": "^3.1.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.14.0",
    "eslint-config-nrk": "^5.0.3-beta.0",
    "eslint-plugin-inferno": "^6.9.2",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^2.28.0",
    "inferno": "1.2.x",
    "inferno-component": "1.2.x",
    "inferno-create-element": "1.2.x",
    "inferno-mobx": "1.2.x",
    "mobx": "^3.1.0",
    "mocha": "^3.2.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "koa": "2.x",
    "koa-favicon": "2",
    "koa-send": "^3.3.0"
  }
}
